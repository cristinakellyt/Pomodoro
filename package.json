{
  "name": "timer-productivity",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass assets/sass/main.scss assets/css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "create_main_css": "mkdir -p assets/  css && touch assets/css/style.css",
    "compile:sass": "node-sass assets/sass/main.scss assets/css/style.comp.css",
    "concat:css": "concat -o assets/css/style.concat.css assets/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" assets/css/style.concat.css -o assets/css/style.prefix.css",
    "compress:css": "node-sass assets/css/style.prefix.css assets/css/style.css --output-style compressed",
    "build:css": "npm-run-all create_main_css compile:sass concat:css prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cristinakellyt/Productivity-timer.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cristinakellyt/Productivity-timer/issues"
  },
  "homepage": "https://github.com/cristinakellyt/Productivity-timer#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "concat": "^1.0.3",
    "node-sass": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.19",
    "postcss-cli": "^10.1.0"
  }
}
